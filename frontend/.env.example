# CS2 Log Generator Frontend Configuration Template
# Copy this to .env and configure for your environment

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_WS_BASE_URL=ws://localhost:8080

# Application Configuration
VITE_APP_TITLE="CS2 Log Generator"
VITE_APP_VERSION="0.1.0"

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_OFFLINE_MODE=false

# Request Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Development Configuration
VITE_DEV_MOCK_API=false
VITE_DEV_SLOW_NETWORK=false
VITE_DEV_FORCE_ERROR=false